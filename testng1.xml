<?xml version="1.0" encoding="UTF-8"?>
<suite name="Test-methodSuite" parallel="methods" thread-count="5">
    <test name="Test-method test" group-by-instances="true">
        <classes>
            <class name="Lecture10.examples.ParametersTests">
                <methods>
                    <include name="testSingleParameter">
                        <parameter name="full-name" value="Harry Potter"></parameter>
                    </include>
                    <include name="testMultiParameters">
                        <parameter name="a" value="1"></parameter>
                        <parameter name="b" value="2"></parameter>
                    </include>
                    <include name="testOptionalParameter"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>